#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x9131580 .scope module, "counter_tb" "counter_tb" 2 8;
 .timescale 0 0;
v0x91516f8_0 .var "clk", 0 0;
v0x9151778_0 .net "counter_out", 3 0, v0x91515d0_0; 1 drivers
v0x91517e8_0 .var "enable", 0 0;
v0x9151858_0 .var "reset", 0 0;
S_0x9131600 .scope module, "connect_counter" "counter" 2 38, 3 8, S_0x9131580;
 .timescale 0 0;
v0x9131700_0 .net "clk", 0 0, v0x91516f8_0; 1 drivers
v0x91515d0_0 .var "counter_out", 3 0;
v0x9151630_0 .net "enable", 0 0, v0x91517e8_0; 1 drivers
v0x9151690_0 .net "reset", 0 0, v0x9151858_0; 1 drivers
E_0x91533c8 .event posedge, v0x9131700_0;
S_0x9131680 .scope begin, "COUNTER" "COUNTER" 3 38, 3 38, S_0x9131600;
 .timescale 0 0;
    .scope S_0x9131600;
T_0 ;
    %wait E_0x91533c8;
    %fork t_1, S_0x9131680;
    %jmp t_0;
    .scope S_0x9131680;
t_1 ;
    %load/v 8, v0x9151690_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 4, 0;
    %assign/v0 v0x91515d0_0, 1, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x9151630_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v0x91515d0_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v0x91515d0_0, 1, 8;
T_0.2 ;
T_0.1 ;
    %end;
    .scope S_0x9131600;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_0x9131580;
T_1 ;
    %delay 5, 0;
    %load/v 8, v0x91516f8_0, 1;
    %inv 8, 1;
    %set/v v0x91516f8_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x9131580;
T_2 ;
    %vpi_call 2 24 "$dumpfile", "counter_wave.vcd";
    %vpi_call 2 25 "$dumpvars", 1'sb0, S_0x9131580;
    %vpi_call 2 27 "$display", "time\011 clk reset enable counter";
    %vpi_call 2 28 "$monitor", "%g\011 %b %b %b %b", $time, v0x91516f8_0, v0x9151858_0, v0x91517e8_0, v0x9151778_0;
    %set/v v0x91516f8_0, 1, 1;
    %set/v v0x9151858_0, 0, 1;
    %delay 5, 0;
    %set/v v0x9151858_0, 1, 1;
    %delay 10, 0;
    %set/v v0x9151858_0, 0, 1;
    %delay 5, 0;
    %set/v v0x91517e8_0, 1, 1;
    %delay 100, 0;
    %set/v v0x91517e8_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 35 "$finish";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
