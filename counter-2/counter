#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x863e298 .scope module, "counter_tb" "counter_tb" 2 8;
 .timescale 0 0;
v0x865e408_0 .var "clk", 0 0;
v0x865e488_0 .net "counter_out", 3 0, v0x865e2e0_0; 1 drivers
v0x865e4f8_0 .var "enable", 0 0;
v0x865e568_0 .var "reset", 0 0;
S_0x863e318 .scope module, "connect_counter" "counter" 2 34, 3 8, S_0x863e298;
 .timescale 0 0;
v0x863e418_0 .net "clk", 0 0, v0x865e408_0; 1 drivers
v0x865e2e0_0 .var "counter_out", 3 0;
v0x865e340_0 .net "enable", 0 0, v0x865e4f8_0; 1 drivers
v0x865e3a0_0 .net "reset", 0 0, v0x865e568_0; 1 drivers
E_0x86603d8 .event posedge, v0x863e418_0;
S_0x863e398 .scope begin, "COUNTER" "COUNTER" 3 38, 3 38, S_0x863e318;
 .timescale 0 0;
    .scope S_0x863e318;
T_0 ;
    %wait E_0x86603d8;
    %fork t_1, S_0x863e398;
    %jmp t_0;
    .scope S_0x863e398;
t_1 ;
    %load/v 8, v0x865e3a0_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 4, 0;
    %assign/v0 v0x865e2e0_0, 1, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x865e340_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v0x865e2e0_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v0x865e2e0_0, 1, 8;
T_0.2 ;
T_0.1 ;
    %end;
    .scope S_0x863e318;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_0x863e298;
T_1 ;
    %delay 5, 0;
    %load/v 8, v0x865e408_0, 1;
    %inv 8, 1;
    %set/v v0x865e408_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x863e298;
T_2 ;
    %vpi_call 2 23 "$display", "time\011 clk reset enable counter";
    %vpi_call 2 24 "$monitor", "%g\011 %b %b %b %b", $time, v0x865e408_0, v0x865e568_0, v0x865e4f8_0, v0x865e488_0;
    %set/v v0x865e408_0, 1, 1;
    %set/v v0x865e568_0, 0, 1;
    %delay 5, 0;
    %set/v v0x865e568_0, 1, 1;
    %delay 10, 0;
    %set/v v0x865e568_0, 0, 1;
    %delay 5, 0;
    %set/v v0x865e4f8_0, 1, 1;
    %delay 100, 0;
    %set/v v0x865e4f8_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 31 "$finish";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
